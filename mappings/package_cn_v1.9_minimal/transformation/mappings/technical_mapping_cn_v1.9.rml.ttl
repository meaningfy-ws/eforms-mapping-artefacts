    rr:predicateObjectMap
        [
            rdfs:label "Aggregate Address" ;
            rdfs:comment "Aggregate values of MG-Address under ND-CompanyAddress" ;
            rr:predicate cv:registeredAddress ;
            rr:objectMap
                [

            rr:class epo:Identifier
        ] ;
    rr:predicateObjectMap
        [
            rdfs:label "identifier" ;
            rr:predicate skos:notation ;
            rr:objectMap
                [
                    rml:reference "cac:PartyIdentification/cbc:ID" ;

  epo:definesSpecificPlaceOfPerformance epd:id_184f8152-752a-453d-9780-e667cc15d6ca_ContractLocation_hM8neAaQhxtDsQ6JVKqiMJ;
  epo:hasAdditionalContractNature <http://publications.europa.eu/resource/authority/contract-nature/services>;

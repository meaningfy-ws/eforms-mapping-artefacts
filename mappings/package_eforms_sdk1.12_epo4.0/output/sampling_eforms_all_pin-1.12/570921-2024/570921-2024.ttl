epd:id_d99dd2ea-4090-46ec-ac73-19f02af4acf2_ContractTerm_4iruvDRR7tf8DCKqYnLNsY a
    epo:ContractTerm;
  epo:definesContractDuration epd:id_d99dd2ea-4090-46ec-ac73-19f02af4acf2_ContractDuration_PmzxxRp53u7KArUjVURPoV;

  epo:hasMainFeature "Sotsiaal- ja eriteenuste erimenetlus"@et;
  epo:hasProcurementScopeDividedIntoLot epd:id_3e47fde5-edfe-4f4f-a130-3486214b23b2_Lot_LOT-0000;

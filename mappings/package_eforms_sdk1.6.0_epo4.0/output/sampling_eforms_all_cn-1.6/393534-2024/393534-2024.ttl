  a epo:SubmissionTerm;
  epo:definesTenderProcessor epd:id_52dd8ba0-d498-4350-899d-1b7734400646_TenderProcessor_AB2DnFEuMtE2L6jgjrbMsy;
  epo:definesTenderReceiver epd:id_52dd8ba0-d498-4350-899d-1b7734400646_TenderReceiver_AB2DnFEuMtE2L6jgjrbMsy;

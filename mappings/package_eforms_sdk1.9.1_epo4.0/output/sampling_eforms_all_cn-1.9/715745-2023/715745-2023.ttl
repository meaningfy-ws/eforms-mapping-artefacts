  epo:definesTenderProcessor epd:id_26a461f0-22f4-4e11-9f5b-39810a6fc437_TenderProcessor_PbuuQVDAP9jvgtF6eprdW2;
  epo:definesTenderReceiver epd:id_26a461f0-22f4-4e11-9f5b-39810a6fc437_TenderReceiver_PbuuQVDAP9jvgtF6eprdW2;

# A resource that has more than one incoming relationship of the same type is
# likely an outlier, except in certain cases (roles playedBy orgs in ePO).

SELECT DISTINCT ?s (?p0 AS ?rel) {
    ?s ?p ?o .
    ?s0 ?p0 ?s .
    ?s1 ?p0 ?s .
    FILTER(isIRI(?s))
    FILTER(?s0 != ?s1)
    FILTER(?p0 not in (epo:playedBy))
}
